{
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "requireConfigFile": false,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "rules": {
        "no-unused-vars": "warn"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ]
}